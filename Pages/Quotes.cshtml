@page
@model myWebApp7475.Pages.QuotesModel
@{
    ViewData["Title"] = "Quotes";
}
    <br />
    <br />
<h1>Quotes</h1>
<h3>From Chernobly </h3>
<br />
<div id="carouselControls" class="carousel slide" data-ride="carousel">
    <div class="carousel-inner">
        <div class="carousel-item active">
            <img class="d-block w-100 h-75" src="~/Images/71.jpg" alt="First slide">
        </div>
        <div class="carousel-item">
            <img class="d-block w-100 h-75" src="~/Images/72.jpg" alt="Second slide">
        </div>
        <div class="carousel-item">
            <img class="d-block w-100 h-75" src="~/Images/73.jpg" alt="Third slide">
        </div>
        <div class="carousel-item">
            <img class="d-block w-100 h-75" src="~/Images/74.jpg" alt="Third slide">
        </div>
        <div class="carousel-item">
            <img class="d-block w-100 h-75" src="~/Images/75.jpg" alt="Third slide">
        </div>
    </div>
    <a class="carousel-control-prev" href="#carouselControls" role="button" data-slide="prev">
        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
        <span class="sr-only">Previous</span>
    </a>
    <a class="carousel-control-next" href="#carouselControls" role="button" data-slide="next">
        <span class="carousel-control-next-icon" aria-hidden="true"></span>
        <span class="sr-only">Next</span>
    </a>
</div>
